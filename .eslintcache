itialise the schema for the data\r\n    const userInitialised = await db.collection('users').doc(user.uid).set(userSchemaObject(user.uid))\r\n    return userInitialised;\r\n  }\r\n\r\n  async function signIn(email, password){\r\n    const {user} = await auth.signInWithEmailAndPassword(email, password)\r\n    return user;\r\n  }\r\n\r\n  const value = {\r\n    // currentUserData,\r\n    currentUser,\r\n    signIn,\r\n    signUp\r\n  }\r\n\r\n  return (\r\n    <AuthContext.Provider value={value}>\r\n      {props.children}\r\n    </AuthContext.Provider>\r\n \r\n  )\r\n}\r\n\r\n// This all works but some things should be added such as\r\n// 1. validation & error messages\r\n// 2. forgot password section\r\n// 3. theres a ton to do for this to be honest....","C:\\pwa-habit\\src\\functions\\schemas\\userSchema.js",[],"C:\\pwa-habit\\src\\functions\\schemas\\habitSchema.js",[],"D:\\Websites\\pwa-habit-rollback\\src\\index.js",[],"D:\\Websites\\pwa-habit-rollback\\src\\App.js",["125","126"],"D:\\Websites\\pwa-habit-rollback\\src\\swDev.js",[],"D:\\Websites\\pwa-habit-rollback\\src\\context\\GlobalProvider.js",["127"],["128","129"],"D:\\Websites\\pwa-habit-rollback\\src\\pages\\components\\Bar.js",[],"D:\\Websites\\pwa-habit-rollback\\src\\pages\\dashboard\\Dashboard.js",["130","131","132","133"],"import React, {useContext, useEffect} from 'react'\r\nimport { Button } from '@material-ui/core';\r\nimport Typography from '@material-ui/core/Typography';\r\nimport Styled from 'styled-components'\r\n\r\n\r\nexport default function Dashboard({habits, setHabits}) {\r\n \r\n\r\n  return (\r\n\r\n    <div>\r\n      <Typography variant=\"h3\" align=\"center\">Dashboard</Typography>\r\n      <div>\r\n        {\r\n          habits.map((h,i)=> <p key={i}>{h.name}</p>)\r\n        }\r\n      </div>\r\n\r\n    </div>\r\n\r\n  )\r\n}\r\n","D:\\Websites\\pwa-habit-rollback\\src\\pages\\habitsArchive\\HabitsArchive.js",["134"],"D:\\Websites\\pwa-habit-rollback\\src\\context\\initialState.js",[